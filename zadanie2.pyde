def setup():
    size(400,400)

def draw(): 
    
    if mousePressed:
        fill(255, 0 , 0);
        rect(100, 100, 80, 80)
        
    if mousePressed:
        fill(255, 0 , 0);
        rect(250, 100, 80, 80)
        
    if mousePressed:
        fill(255, 0 , 0);
        rect(140, 250, 150, 15)
        
    if mousePressed:
        fill(255, 0 , 0);
        rect(140, 250, 150, 15)
    else:
        background(255, 200, 200)
